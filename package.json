{
  "name": "nestro-ui-kit",
  "version": "0.7.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint --ext .js,.ts,.vue src",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,md}\""
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-onboarding": "^8.6.9",
    "@storybook/blocks": "^8.6.9",
    "@storybook/experimental-addon-test": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@storybook/vue3": "^8.6.9",
    "@storybook/vue3-vite": "^8.6.9",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "playwright": "^1.51.1",
    "prettier": "^3.2.5",
    "sass": "^1.89.2",
    "storybook": "^8.6.9",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.4"
  },
  "main": "dist/nestro-ui-kit.umd.js",
  "module": "dist/nestro-ui-kit.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/nestro-ui-kit.es.js",
      "require": "./dist/nestro-ui-kit.umd.js"
    },
    "./nestro-ui-kit.css": "./dist/nestro-ui-kit.css"
  }
